#!/bin/bash

if [ $# -eq 0 ]; then
  echo "Please provide an INSTANCE_NAME to unbind"
  exit 1
fi
instance_name=$1

source base
trap "log_output \"[OWNER] ðŸš¦ Stopping guest.unbind script\"; exit 1" SIGINT

log_output "[HOST] ðŸ’¤ Unbinding Guest machine: $instance_name"
